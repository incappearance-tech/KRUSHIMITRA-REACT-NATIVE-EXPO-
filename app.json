{
  "expo": {
    "name": "KRUSHIMITRA-REACT-NATIVE-EXPO-",
    "slug": "krushimitra",
    "version": "1.0.0",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "sdkVersion": "54.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4f8ce05d-02e4-472d-b21e-ef0139acfba8"
      }
    },
    "orientation": "portrait",
    "scheme": "krushimitrareactnativeexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.krushimitra.app"
    },
    "android": {
      "package": "com.krushimitra.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}